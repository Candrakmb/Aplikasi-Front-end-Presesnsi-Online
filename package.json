{
  "name": "vue-onsenui-kitchensink",
  "version": "0.0.1",
  "description": "Kitchen Sink example for Onsen UI 2 Vue extension.",
  "author": "Fran Dios <me@frandiox.com>",
  "scripts": {
    "build": "cross-env NODE_ENV=production webpack --progress --hide-modules",
    "deploy": "npm run build && npm run demo:index && gh-pages -d www",
    "build:watch": "cross-env NODE_ENV=production WEBPACK_WATCH=true webpack --progress --hide-modules",
    "demo:index": "mv dist/index.html public/",
    "dev": "cross-env NODE_ENV=development webpack-dev-server --open --hot --host localhost --port 7000"
  },
  "dependencies": {
    "axios": "^1.4.0",
    "babel-plugin-transform-runtime": "^6.23.0",
    "bcryptjs": "^2.4.3",
    "gh-pages": "^5.0.0",
    "leaflet": "^1.9.3",
    "onsenui": "^2.10.2",
    "register-service-worker": "^1.7.2",
    "vue": "^2.5.3",
    "vue-onsenui": "^2.6.1",
    "vue2-leaflet": "^2.7.1",
    "vuex": "^3.0.1"
  },
  "devDependencies": {
    "@vue/cli-plugin-pwa": "~5.0.0",
    "babel-core": "^6.0.0",
    "babel-loader": "^7.1.5",
    "babel-plugin-external-helpers": "^6.22.0",
    "babel-preset-env": "^1.6.1",
    "copy-webpack-plugin": "^4.0.1",
    "cross-env": "^3.0.0",
    "css-loader": "^0.28.7",
    "extract-text-webpack-plugin": "^2.1.0",
    "file-loader": "^0.11.2",
    "html-webpack-plugin": "^2.28.0",
    "postcss": "^6.0.11",
    "postcss-base64": "^0.7.1",
    "postcss-cssnext": "^3.0.2",
    "postcss-loader": "^2.0.6",
    "postcss-smart-import": "^0.7.5",
    "postcss-url": "^7.1.2",
    "style-loader": "^0.18.2",
    "vue-loader": "^12.0.4",
    "vue-template-compiler": "^2.5.3",
    "webpack": "^3.8.1",
    "webpack-dev-server": "2.7.1"
  },
  "license": "MIT"
}
